{
  "name": "react-budget",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "cleave.js": "1.3.7",
    "emotion": "9.1.3",
    "numeral": "2.0.6",
    "prop-types": "15.6.1",
    "react": "^16.4.0",
    "react-app-rewire-hot-loader": "1.0.1",
    "react-app-rewired": "1.5.2",
    "react-dom": "^16.4.0",
    "react-emotion": "9.1.3",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "redux-actions": "2.4.0",
    "reselect": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start & npm run start-server",
    "now-start": "npm run build && mv build server/build && cd server && npm install && npm start",
    "start-server": "cd server && npm start",
    "build": "react-scripts build",
    "deploy": "npm run build && cd build && rm static/{css,js}/*.map && now --public",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "update-snapshots": "CI=true react-scripts test --env=jsdom --updateSnapshot",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addon-links": "^3.4.7",
    "@storybook/addons": "^3.4.7",
    "@storybook/cli": "3.4.7",
    "@storybook/react": "^3.4.7",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "express-generator": "4.16.0",
    "jest-emotion": "9.2.2",
    "jest-localstorage-mock": "2.2.0",
    "react-hot-loader": "4.2.0"
  }
}
